{
  "name": "@trentpierce/browser-agent",
  "version": "2.0.0",
  "description": "An intelligent AI-powered browser automation library with multi-LLM support, visual understanding, and adaptive learning",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "browser-agent": "bin/browser-agent.js"
  },
  "scripts": {
    "start": "electron .",
    "start:standalone": "node bin/browser-agent.js standalone",
    "start:server": "node bin/browser-agent.js server",
    "test": "jest",
    "build": "npm run build:docs",
    "build:docs": "jsdoc -c jsdoc.json",
    "prepublishOnly": "npm test",
    "docker:build": "docker build -t browser-agent .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "browser-automation",
    "ai",
    "llm",
    "gemini",
    "openai",
    "anthropic",
    "stagehand",
    "electron",
    "puppeteer",
    "web-automation",
    "machine-learning",
    "adaptive-learning",
    "visual-understanding",
    "mcp",
    "cloud",
    "api",
    "websocket"
  ],
  "author": "Trent Pierce",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrentPierce/BrowserAgent.git"
  },
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^16.4.5",
    "keytar": "^7.9.0",
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "puppeteer": "^21.5.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "electron": "^29.0.0",
    "jest": "^29.7.0",
    "@types/node": "^20.10.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "@anthropic-ai/sdk": "^0.9.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.js"
    },
    "./api": "./src/api/index.js",
    "./providers": "./src/providers/index.js",
    "./tools": "./src/tools/index.js"
  }
}