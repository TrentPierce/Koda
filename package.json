{
  "name": "@trentpierce/browser-agent",
  "version": "2.2.0",
  "description": "An intelligent AI-powered browser automation library with computer vision, distributed execution, enterprise scaling, accessibility testing, performance monitoring, and security testing capabilities",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "browser-agent": "bin/browser-agent.js"
  },
  "scripts": {
    "start": "electron .",
    "start:standalone": "node bin/browser-agent.js standalone",
    "start:server": "node bin/browser-agent.js server",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint \"src/**/*.js\" || exit 0",
    "lint:fix": "eslint \"src/**/*.js\" --fix",
    "lint:strict": "eslint \"src/**/*.js\"",
    "build": "npm run build:docs",
    "build:docs": "jsdoc -c jsdoc.json || echo 'JSDoc build skipped'",
    "prepublishOnly": "npm run lint:strict && npm test",
    "docker:build": "docker build -t browser-agent .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "browser-automation",
    "computer-vision",
    "distributed-execution",
    "load-balancing",
    "accessibility-testing",
    "performance-monitoring",
    "security-testing",
    "form-fuzzing",
    "mobile-automation",
    "ios-automation",
    "android-automation",
    "appium",
    "reinforcement-learning",
    "q-learning",
    "policy-gradient",
    "ai",
    "llm",
    "gemini",
    "openai",
    "anthropic",
    "claude",
    "electron",
    "puppeteer",
    "web-automation",
    "machine-learning",
    "adaptive-learning",
    "visual-understanding",
    "pattern-recognition",
    "enterprise",
    "wcag",
    "a11y"
  ],
  "author": "Trent Pierce",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrentPierce/BrowserAgent.git"
  },
  "bugs": {
    "url": "https://github.com/TrentPierce/BrowserAgent/issues"
  },
  "homepage": "https://github.com/TrentPierce/BrowserAgent#readme",
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.0",
    "axe-core": "^4.10.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "opencv4nodejs": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "keytar": "^7.9.0",
    "puppeteer": "^23.10.0",
    "sharp": "^0.33.5",
    "@wdio/cli": "^8.40.0",
    "webdriverio": "^8.40.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "electron": "^33.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "supertest": "^7.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "appium": "^2.0.0",
    "openai": "^4.73.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "appium": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.js"
    },
    "./api": "./src/api/index.js",
    "./providers": "./src/providers/index.js",
    "./tools": "./src/tools/index.js",
    "./mobile": "./src/mobile/index.js",
    "./learning": "./src/learning/index.js",
    "./vision": "./src/vision/index.js",
    "./enterprise": "./src/enterprise/index.js",
    "./testing": "./src/testing/index.js"
  }
}
