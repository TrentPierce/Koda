{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": [
        "feature",
        "enhancement",
        "feat"
      ]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": [
        "bug",
        "fix",
        "bugfix"
      ]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": [
        "documentation",
        "docs"
      ]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": [
        "chore",
        "maintenance",
        "refactor",
        "dependencies",
        "deps"
      ]
    },
    {
      "title": "## âš¡ Performance",
      "labels": [
        "performance",
        "perf"
      ]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": [
        "security"
      ]
    },
    {
      "title": "## ğŸ§ª Testing",
      "labels": [
        "test",
        "testing"
      ]
    },
    {
      "title": "## ğŸ¨ Styling",
      "labels": [
        "style",
        "styling"
      ]
    }
  ],
  "ignore_labels": [
    "ignore",
    "skip-changelog",
    "wontfix",
    "duplicate",
    "invalid"
  ],
  "sort": "ASC",
  "template": "#{{CHANGELOG}}\n\n**Full Changelog**: #{{RELEASE_DIFF}}\n\n## Contributors\n\nThank you to all contributors who made this release possible!\n",
  "pr_template": "- #{{TITLE}} (#{{NUMBER}}) by @#{{AUTHOR}}",
  "empty_template": "- No changes in this release",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "label_extractor": [
    {
      "pattern": "^feat(\\(.+\\))?:",
      "target": "feature"
    },
    {
      "pattern": "^fix(\\(.+\\))?:",
      "target": "bug"
    },
    {
      "pattern": "^docs(\\(.+\\))?:",
      "target": "documentation"
    },
    {
      "pattern": "^chore(\\(.+\\))?:",
      "target": "chore"
    }
  ]
}